	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-optional-chaining": "^7.12.7",
		"customize-cra": "^1.0.0",
		"gh-pages": "^3.1.0",
		"react-app-rewired": "^2.1.8"
	}
